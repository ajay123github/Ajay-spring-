<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RasipalaluActivity">

<androidx.cardview.widget.CardView
    android:id="@+id/headingcard"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <LinearLayout
        android:id="@+id/heading"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/leftarrow"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/leftarrow"
            android:layout_margin="20sp"
            android:background="@color/red"/>
        <TextView
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Services"
            android:textStyle="bold"
            android:textColor="@color/violet"
            android:textSize="30sp"
            android:layout_margin="20sp"/>
    </LinearLayout>

</androidx.cardview.widget.CardView>

    <LinearLayout
        android:id="@+id/body1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/headingcard"
        android:layout_marginTop="10sp"
        android:layout_marginLeft="10sp">

        <androidx.cardview.widget.CardView
            android:id="@+id/dinacard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/nitracouples"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="20sp"
                    android:text="నిత్ర వివాహం"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/varacard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/homamservices"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="20sp"
                    android:text="HomamServices"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/body2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/body1"
        android:layout_marginTop="10sp"
        android:layout_marginLeft="10sp"
        >

        <androidx.cardview.widget.CardView
            android:id="@+id/maasacard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/jobs"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="25sp"
                    android:text="ఉద్యోగాలు"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/yearcard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/diyapoojastores"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="25sp"
                    android:text="శ్రీ దియా పూజా స్టోర్"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/body3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/body2"
        android:layout_marginTop="10sp"
        android:layout_marginLeft="10sp">

        <androidx.cardview.widget.CardView
            android:id="@+id/englishyearcard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/birth"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="120sp"
                    android:layout_height="20sp"
                    android:text="జన్మ పత్రిక"
                    android:textStyle="bold"
                    android:layout_marginLeft="20sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/rasihistorycard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/onlinemuhurthalu"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="25sp"
                    android:text="Online Muhurthalu"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/body4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/body3"
        android:layout_marginTop="10sp"
        android:layout_marginLeft="10sp">

        <androidx.cardview.widget.CardView
            android:id="@+id/birthcard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/pdficon"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="25sp"
                    android:text="PDF BookStore"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/starcard"
            android:layout_width="155sp"
            android:layout_height="135sp"
            app:cardCornerRadius="10sp"
            app:cardElevation="5dp"
            android:layout_marginLeft="10sp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:src="@drawable/computerjatakam"
                    android:layout_marginLeft="37.5sp"
                    android:layout_marginTop="15sp"/>
                <TextView
                    android:layout_width="80sp"
                    android:layout_height="27sp"
                    android:text="కంప్యూటర్ జాతకం"
                    android:textStyle="bold"
                    android:layout_marginLeft="37.5sp"
                    android:textSize="10sp"
                    android:gravity="center"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>


</RelativeLayout>